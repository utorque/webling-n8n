{
  "name": "n8n-nodes-webling",
  "version": "1.0.0",
  "description": "n8n nodes for Webling API - member management, finance and automation",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "webling",
    "member-management",
    "accounting",
    "finance"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Thibault Schowing",
    "email": "thibault@example.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
